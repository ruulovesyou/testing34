	local ug=Instance.new("UIGradient",crgb)ug.Color=ColorSequence.new{ColorSequenceKeypoint.new(0,Color3.fromRGB(255,0,0)),ColorSequenceKeypoint.new(.1,Color3.fromRGB(255,153,0)),ColorSequenceKeypoint.new(.2,Color3.fromRGB(203,255,0)),ColorSequenceKeypoint.new(.3,Color3.fromRGB(50,255,0)),ColorSequenceKeypoint.new(.4,Color3.fromRGB(0,255,102)),ColorSequenceKeypoint.new(.5,Color3.fromRGB(0,255,255)),ColorSequenceKeypoint.new(.6,Color3.fromRGB(0,101,255)),ColorSequenceKeypoint.new(.7,Color3.fromRGB(50,0,255)),ColorSequenceKeypoint.new(.8,Color3.fromRGB(204,0,255)),ColorSequenceKeypoint.new(.9,Color3.fromRGB(255,0,153)),ColorSequenceKeypoint.new(1,Color3.fromRGB(255,0,0))}ug.Rotation=90
	Instance.new("UICorner",crgb).CornerRadius=UDim.new(1,0)

	local crs=Instance.new("Frame",crgb)crs.Name=Compkiller:_RandomString()crs.AnchorPoint=Vector2.new(.5,0)crs.BackgroundTransparency=1 crs.Position=UDim2.new(.5,0,.5,0)crs.Size=UDim2.new(1,0,0,2)crs.ZIndex=z+7
	local l=Instance.new("Frame",crs)l.BackgroundColor3=Color3.new(1,1,1)l.Size=UDim2.new(0,2,1,0)l.ZIndex=z+100 Instance.new("UIStroke",l)
	local r=Instance.new("Frame",crs)r.AnchorPoint=Vector2.new(1,0)r.BackgroundColor3=Color3.new(1,1,1)r.Position=UDim2.new(1,0,0,0)r.Size=UDim2.new(0,2,1,0)r.ZIndex=z+100 Instance.new("UIStroke",r)

	local cop=Instance.new("Frame",pw)cop.Name=Compkiller:_RandomString()cop.BackgroundColor3=Color3.fromRGB(102,255,0)cop.Position=UDim2.new(0,7,0,160)cop.Size=UDim2.new(1,-30,0,9)cop.ZIndex=z+6
	Instance.new("UICorner",cop).CornerRadius=UDim.new(1,0)
	local cos=Instance.new("Frame",cop)cos.Name=Compkiller:_RandomString()cos.AnchorPoint=Vector2.new(0,.5)cos.BackgroundTransparency=1 cos.Position=UDim2.new(.5,0,.5,0)cos.Size=UDim2.new(0,2,1,0)cos.ZIndex=z+7
	local l2=Instance.new("Frame",cos)l2.BackgroundColor3=Color3.new(1,1,1)l2.Size=UDim2.new(1,0,0,2)l2.ZIndex=z+100 Instance.new("UIStroke",l2)
	local r2=Instance.new("Frame",cos)r2.AnchorPoint=Vector2.new(0,1)r2.BackgroundColor3=Color3.new(1,1,1)r2.Position=UDim2.new(0,0,1,0)r2.Size=UDim2.new(1,0,0,2)r2.ZIndex=z+100 Instance.new("UIStroke",r2)
	Instance.new("UIGradient",cop).Transparency=NumberSequence.new{NumberSequenceKeypoint.new(0,0),NumberSequenceKeypoint.new(1,1)}
	Instance.new("UIStroke",cop).Color=Color3.fromRGB(29,29,29)Instance.new("UIStroke",cop).Transparency=.5

	local ti=Instance.new("ImageLabel",cop)ti.Name=Compkiller:_RandomString()ti.BackgroundTransparency=1 ti.Size=UDim2.new(1,0,1,0)ti.ZIndex=z+5 ti.Image=Compkiller:CacheImage("rbxassetid://6198493000")ti.ImageColor3=Color3.fromRGB(206,206,206)ti.ScaleType=Enum.ScaleType.Crop
	Instance.new("UICorner",ti).CornerRadius=UDim.new(1,0)

	local hf=Instance.new("Frame",pw)hf.Name=Compkiller:_RandomString()hf.AnchorPoint=Vector2.new(.5,1)hf.BackgroundColor3=Compkiller.Colors.BlockColor hf.Position=UDim2.new(.5,0,1,-5)hf.Size=UDim2.new(1,-16,0,20)hf.ZIndex=z+205 table.insert(Compkiller.Elements.BlockColor,{Element=hf,Property="BackgroundColor3"})
	Instance.new("UICorner",hf).CornerRadius=UDim.new(0,4)
	local hs=Instance.new("UIStroke",hf)hs.Color=Compkiller.Colors.HighStrokeColor table.insert(Compkiller.Elements.HighStrokeColor,{Element=hs,Property="Color"})

	local tl=Instance.new("TextLabel",hf)tl.AnchorPoint=Vector2.new(.5,.5)tl.BackgroundTransparency=1 tl.Position=UDim2.new(.5,0,.5,0)tl.Size=UDim2.new(1,-10,1,-5)tl.ZIndex=z+206 tl.Font=Enum.Font.Gotham tl.Text="#FFFFFF" tl.TextColor3=Compkiller.Colors.SwitchColor tl.TextSize=13 tl.TextXAlignment=Enum.TextXAlignment.Left table.insert(Compkiller.Elements.SwitchColor,{Element=tl,Property="TextColor3"})

	local args={IsHold=false,IsVisible=false}
	local t1=TweenInfo.new(.2,Enum.EasingStyle.Quad)local t2=TweenInfo.new(.275,Enum.EasingStyle.Quad)

	Compkiller:_AddPropertyEvent(pw,function(v)pw.Visible=v if Compkiller.PerformanceMode then Compkiller:_SetNilP(pw,v and win or nil)else Compkiller:_SetNilP(pw,win)end end)

	local toggle=function(b)
		if args.IsVisible==b then return end args.IsVisible=b
		local mp=UDim2.new(0,btn.AbsolutePosition.X+95,0,btn.AbsolutePosition.Y+65)local dp=UDim2.new(0,mp.X.Offset,0,mp.Y.Offset+15)
		local mul=win.AbsoluteSize.Y/2
		if mp.Y.Offset>mul then mp=UDim2.fromOffset(btn.AbsolutePosition.X,btn.AbsolutePosition.Y+45)dp=UDim2.fromOffset(mp.X.Offset,mp.Y.Offset-25)pw.AnchorPoint=Vector2.new(.5,1)else pw.AnchorPoint=Vector2.new(.5,0)end
		if b then
			if mp~=dp then pw.Position=dp end
			Compkiller:_Animation(pw,t2,{BackgroundTransparency=0,Size=UDim2.new(0,175,0,200)})
			Compkiller:_Animation(pw,t1,{Position=mp})
			for _,o in{pw,cp,mm,cop,ti,l,l2,r,r2,crgb,hf,tl,hs,Instance.new("UIStroke",cop)}do Compkiller:_Animation(o,t1,{Transparency=o:IsA("UIStroke")and 0 or o:IsA("ImageLabel")and 0 or 0})end
		else
			for _,o in{pw,cp,mm,cop,ti,l,l2,r,r2,crgb,hf,tl,hs,Instance.new("UIStroke",cop)}do Compkiller:_Animation(o,t1,{Transparency=o:IsA("UIStroke")and 1 or o:IsA("ImageLabel")and 1 or 1})end
			Compkiller:_Animation(pw,t2,{BackgroundTransparency=1})Compkiller:_Animation(pw,t1,{Position=dp})
		end
	end

	btn.MouseButton1Click:Connect(function()toggle(true)end)

	local h,s,v,t=0,0,0,0

	function args:SetColor(c,tv)h,s,v=c:ToHSV()t=tv end

	function args:Update()
		local mc=Color3.fromHSV(h,s,1)local rc=Color3.fromHSV(h,s,v)
		Compkiller:_Animation(cp,t1,{BackgroundColor3=Color3.fromHSV(h,1,1)})
		Compkiller:_Animation(cop,t1,{BackgroundColor3=rc})
		Compkiller:_Animation(mm,t1,{Position=UDim2.fromScale(s,1-v)})
		Compkiller:_Animation(cos,t1,{Position=UDim2.new(t,0,.5,0)})
		Compkiller:_Animation(crs,t1,{Position=UDim2.new(.5,0,h,0)})
		tl.Text="#"..rc:ToHex()
		cb(rc,t)
	end

	local th
	pw.InputBegan:Connect(function(i)if i.UserInputType==Enum.UserInputType.MouseButton1 or i.UserInputType==Enum.UserInputType.Touch then args.IsHold=true if th then task.cancel(th)end th=task.spawn(function()while args.IsHold do task.wait()cb(Color3.fromHSV(h,s,v),t)end end)end end)
	pw.InputEnded:Connect(function(i)if i.UserInputType==Enum.UserInputType.MouseButton1 or i.UserInputType==Enum.UserInputType.Touch then args.IsHold=false if th then task.cancel(th)th=nil end end end)

	UserInputService.InputBegan:Connect(function(i)if(i.UserInputType==Enum.UserInputType.MouseButton1 or i.UserInputType==Enum.UserInputType.Touch)and not Compkiller:_IsMouseOverFrame(pw)then toggle(false)end end)

	crgb.InputBegan:Connect(function(i)if i.UserInputType==Enum.UserInputType.MouseButton1 or i.UserInputType==Enum.UserInputType.Touch then args.IsHold=true while args.IsHold do task.wait()local y=crgb.AbsolutePosition.Y local ym=y+crgb.AbsoluteSize.Y local c=math.clamp(Mouse.Y,y,ym)h=(c-y)/(ym-y)args:Update()end end end)
	cop.InputBegan:Connect(function(i)if i.UserInputType==Enum.UserInputType.MouseButton1 or i.UserInputType==Enum.UserInputType.Touch then args.IsHold=true while args.IsHold do task.wait()t=math.clamp((Mouse.X-cop.AbsolutePosition.X)/cop.AbsoluteSize.X,0,1)args:Update()end end end)
	cp.InputBegan:Connect(function(i)if i.UserInputType==Enum.UserInputType.MouseButton1 or i.UserInputType==Enum.UserInputType.Touch then args.IsHold=true while args.IsHold do task.wait()local x=cp.AbsolutePosition.X local sx=x+cp.AbsoluteSize.X local y=cp.AbsolutePosition.Y local sy=y+cp.AbsoluteSize.Y s=(math.clamp(Mouse.X,x,sx)-x)/(sx-x)v=1-((math.clamp(Mouse.Y,y,sy)-y)/(sy-y))args:Update()end end end)

	return args
end










function Compkiller:_DrawKeybinds(win)
	if Compkiller.__KEYBINDS_CACHE then return Compkiller.__KEYBINDS_CACHE end
	local kb=Instance.new("Frame",win)kb.Name=Compkiller:_RandomString()kb.BackgroundColor3=Compkiller.Colors.BGDBColor kb.BackgroundTransparency=.025 kb.Position=UDim2.new(0,100,0,100)kb.Size=UDim2.new(0,125,0,25)kb.ZIndex=150
	table.insert(Compkiller.Elements.BGDBColor,{Element=kb,Property="BackgroundColor3"})
	Instance.new("UICorner",kb).CornerRadius=UDim.new(0,3)
	local icf=Instance.new("Frame",kb)icf.AnchorPoint=Vector2.new(1,.5)icf.BackgroundColor3=Compkiller.Colors.BGDBColor icf.BackgroundTransparency=.3 icf.Position=UDim2.new(0,5,.5,0)icf.Size=UDim2.new(1,10,1,0)icf.SizeConstraint=Enum.SizeConstraint.RelativeYY icf.ZIndex=149
	table.insert(Compkiller.Elements.BGDBColor,{Element=icf,Property="BackgroundColor3"})
	Instance.new("UICorner",icf).CornerRadius=UDim.new(0,3)
	local fr=Instance.new("Frame",icf)fr.AnchorPoint=Vector2.new(0,.5)fr.BackgroundColor3=Compkiller.Colors.Highlight fr.Position=UDim2.new(1,-5,.5,0)fr.Size=UDim2.new(0,2,1,0)fr.ZIndex=151
	table.insert(Compkiller.Elements.Highlight,{Element=fr,Property="BackgroundColor3"})
	local ico=Instance.new("ImageLabel",icf)ico.AnchorPoint=Vector2.new(.5,.5)ico.BackgroundTransparency=1 ico.Position=UDim2.new(.5,-2,.5,0)ico.Size=UDim2.new(0,20,0,20)ico.SizeConstraint=Enum.SizeConstraint.RelativeYY ico.ZIndex=159 ico.Image=Compkiller:CacheImage("rbxassetid://10723416765")
	local hf=Instance.new("Frame",kb)hf.AnchorPoint=Vector2.new(.5,0)hf.BackgroundTransparency=1 hf.ClipsDescendants=true hf.Position=UDim2.new(.5,0,0,0)hf.Size=UDim2.new(1,-10,1,0)hf.ZIndex=155
	local hl=Instance.new("TextLabel",hf)hl.AnchorPoint=Vector2.new(.5,.5)hl.BackgroundTransparency=1 hl.Position=UDim2.new(.5,0,.5,0)hl.Size=UDim2.new(1,-10,1,0)hl.ZIndex=156 hl.Font=Enum.Font.GothamMedium hl.Text="Keybinds" hl.TextColor3=Compkiller.Colors.SwitchColor hl.TextSize=12
	table.insert(Compkiller.Elements.SwitchColor,{Element=hl,Property="TextColor3"})
	local mf=Instance.new("Frame",kb)mf.AnchorPoint=Vector2.new(1,0)mf.BackgroundTransparency=1 mf.Position=UDim2.new(1,0,1,5)mf.Size=UDim2.new(1,30,1,3)mf.ZIndex=156 mf.ClipsDescendants=true
	local ul=Instance.new("UIListLayout",mf)ul.HorizontalAlignment=Enum.HorizontalAlignment.Center ul.SortOrder=Enum.SortOrder.LayoutOrder ul.Padding=UDim.new(0,5)
	local mvf=Instance.new("Frame",kb)mvf.AnchorPoint=Vector2.new(1,.5)mvf.BackgroundTransparency=1 mvf.Position=UDim2.new(1,0,.5,0)mvf.Size=UDim2.new(1,30,1,0)
	Compkiller:Drag(mvf,kb,.1)
	local ref={Root=kb}
	ref.THREAD=task.spawn(function()
		while true do task.wait()
			Compkiller:_Animation(mf,TweenInfo.new(.4),{Size=UDim2.new(1,30,1,ul.AbsoluteContentSize.Y+1)})
			if ul.AbsoluteContentSize.Y>1 then
				Compkiller:_Animation(icf,TweenInfo.new(.25),{BackgroundTransparency=.3})
				Compkiller:_Animation(fr,TweenInfo.new(.25),{BackgroundTransparency=0})
				Compkiller:_Animation(hl,TweenInfo.new(.25),{TextTransparency=0})
				Compkiller:_Animation(ico,TweenInfo.new(.25),{ImageTransparency=0})
				local mx=100 for _,c in mf:GetChildren()do if c:GetAttribute("AvgScale")and c:GetAttribute("AvgScale")>mx then mx=c:GetAttribute("AvgScale")end end
				Compkiller:_Animation(kb,TweenInfo.new(.25),{BackgroundTransparency=.025,Size=UDim2.new(0,mx,0,25)})
			else
				Compkiller:_Animation(hl,TweenInfo.new(.25),{TextTransparency=1})
				Compkiller:_Animation(kb,TweenInfo.new(.25),{BackgroundTransparency=1})
				Compkiller:_Animation(icf,TweenInfo.new(.25),{BackgroundTransparency=1})
				Compkiller:_Animation(fr,TweenInfo.new(.25),{BackgroundTransparency=1})
				Compkiller:_Animation(ico,TweenInfo.new(.25),{ImageTransparency=1})
			end
			kb.Visible=kb.BackgroundTransparency<.9
			if Compkiller.PerformanceMode then Compkiller:_SetNilP(kb,kb.Visible and win or nil)else Compkiller:_SetNilP(kb,win)end
		end
	end)
	function ref:AddFrame()
		local kh=Instance.new("Frame")kh.Name=Compkiller:_RandomString()kh.BackgroundColor3=Compkiller.Colors.BGDBColor kh.BackgroundTransparency=1 kh.Size=UDim2.new(1,0,0,28)kh.ZIndex=mf.ZIndex+3 kh.ClipsDescendants=true
		table.insert(Compkiller.Elements.BGDBColor,{Element=kh,Property="BackgroundColor3"})
		Instance.new("UICorner",kh).CornerRadius=UDim.new(0,3)
		local lb=Instance.new("TextLabel",kh)lb.AnchorPoint=Vector2.new(.5,.5)lb.BackgroundTransparency=1 lb.Position=UDim2.new(.5,0,.5,0)lb.Size=UDim2.new(1,-10,1,0)lb.ZIndex=mf.ZIndex+5 lb.Font=Enum.Font.GothamMedium lb.TextColor3=Compkiller.Colors.SwitchColor lb.TextSize=11 lb.TextTransparency=1 lb.TextXAlignment=Enum.TextXAlignment.Left
		table.insert(Compkiller.Elements.SwitchColor,{Element=lb,Property="TextColor3"})
		local ln=Instance.new("Frame",kh)ln.AnchorPoint=Vector2.new(1,.5)ln.BackgroundColor3=Compkiller.Colors.BGDBColor ln.Position=UDim2.new(1,0,.5,0)ln.Size=UDim2.new(0,30,1,0)ln.ZIndex=mf.ZIndex+4
		table.insert(Compkiller.Elements.BGDBColor,{Element=ln,Property="BackgroundColor3"})
		local tl=Instance.new("TextLabel",ln)tl.AnchorPoint=Vector2.new(.5,.5)tl.BackgroundTransparency=1 tl.Position=UDim2.new(.5,0,.5,0)tl.Size=UDim2.new(1,0,1,0)tl.ZIndex=mf.ZIndex+6 tl.Font=Enum.Font.GothamMedium tl.TextColor3=Compkiller.Colors.SwitchColor tl.TextSize=11
		table.insert(Compkiller.Elements.SwitchColor,{Element=tl,Property="TextColor3"})
		Instance.new("UICorner",ln).CornerRadius=UDim.new(0,3)
		local up=function()
			local ts=TextService:GetTextSize(tl.Text,tl.TextSize,tl.Font,Vector2.new(math.huge,math.huge))
			local ls=TextService:GetTextSize(lb.Text,lb.TextSize,lb.Font,Vector2.new(math.huge,math.huge))
			ln.Size=UDim2.new(0,ts.X+5,1,0)kh:SetAttribute("AvgScale",ts.X+ls.X+55)
		end up()
		local fr={SetName=function(s)lb.Text=s or lb.Text up()end,SetType=function(s)tl.Text=s or tl.Text up()end,
			SetVisible=function(v)
				if v then
					Compkiller:_Animation(kh,TweenInfo.new(.1),{BackgroundTransparency=.6,Size=UDim2.new(1,0,0,28)})
					Compkiller:_Animation(lb,TweenInfo.new(.15),{TextTransparency=.1})
					Compkiller:_Animation(ln,TweenInfo.new(.15),{BackgroundTransparency=0})
					Compkiller:_Animation(tl,TweenInfo.new(.15),{TextTransparency=0})
				else
					Compkiller:_Animation(kh,TweenInfo.new(.1),{BackgroundTransparency=1,Size=UDim2.new(1,0,0,0)})
					Compkiller:_Animation(lb,TweenInfo.new(.15),{TextTransparency=1})
					Compkiller:_Animation(ln,TweenInfo.new(.15),{BackgroundTransparency=1})
					Compkiller:_Animation(tl,TweenInfo.new(.15),{TextTransparency=1})
				end
				kh.Parent=v and mf or nil up()
			end}
		return fr
	end
	Compkiller.__KEYBINDS_CACHE=ref return ref
end



--main

function Compkiller.new(Config : Window)
    local UI_TITLE = "CompKiller_Interface"

    -- Check for existing GUI and remove it
    if game:GetService("CoreGui"):FindFirstChild(UI_TITLE) then
        game:GetService("CoreGui")[UI_TITLE]:Destroy()
    end

    if not Config.Scale then
        if Compkiller:_IsMobile() then
            Config.Scale = Compkiller.Scale.Mobile
        else
            Config.Scale = Compkiller.Scale.Window
        end
    end

    Config = Compkiller.__CONFIG(Config , {
        Name       = "COMPKILLER",
        Keybind    = "Insert",
        Logo       = Compkiller.Logo,
        Scale      = Compkiller.Scale.Window,
        TextSize   = 15
    })

    local TabHover       = Compkiller.__SIGNAL(false)
    local WindowOpen     = Compkiller.__SIGNAL(true)

    local WindowArgs = {
        SelectedTab   = nil,
        Tabs          = {},
        LastTab       = nil,
        IsOpen        = true,
        AlwayShowTab  = false,
        THREADS       = {},
        PerformanceMode = false,
        Notify        = Compkiller.newNotify()
    }

    WindowArgs.Username = LocalPlayer.Name

    if Compkiller:_IsMobile() then
        WindowArgs.AlwayShowTab = true
    end

    -- // UI Creation
    local CompKiller              = Instance.new("ScreenGui")
    local MainFrame               = Instance.new("Frame")
    local UICorner                = Instance.new("UICorner")
    local TabFrame                = Instance.new("Frame")
    local UICorner_2              = Instance.new("UICorner")
    local LineFrame1              = Instance.new("Frame")
    local CompLogo                = Instance.new("ImageLabel")
    local WindowLabel             = Instance.new("TextLabel")
    local TabButtons              = Instance.new("Frame")
    local SelectionFrame          = Instance.new("Frame")
    local UICorner_3              = Instance.new("UICorner")
    local TabButtonScrollingFrame = Instance.new("ScrollingFrame")
    local UIListLayout            = Instance.new("UIListLayout")
    local Userinfo                = Instance.new("Frame")
    local UserProfile             = Instance.new("ImageLabel")
    local UICorner_4              = Instance.new("UICorner")
    local UserText                = Instance.new("TextLabel")
    local ExpireText              = Instance.new("TextLabel")
    local TabMainFrame            = Instance.new("Frame")

    Compkiller:_DrawKeybinds(CompKiller)

    UIListLayout:GetPropertyChangedSignal('AbsoluteContentSize'):Connect(function()
        TabButtonScrollingFrame.CanvasSize = UDim2.fromOffset(0, UIListLayout.AbsoluteContentSize.Y)
    end)

    -- Set static name for cleanup logic
    CompKiller.Name             = UI_TITLE
    CompKiller.Parent           = CoreGui
    CompKiller.ResetOnSpawn     = false
    CompKiller.IgnoreGuiInset   = true
    CompKiller.ZIndexBehavior   = Enum.ZIndexBehavior.Global
    Compkiller.ProtectGui(CompKiller)

    WindowArgs.Root = CompKiller
    table.insert(Compkiller.Windows , CompKiller)

    MainFrame.Active            = true
    MainFrame.Name              = Compkiller:_RandomString()
    MainFrame.Parent            = CompKiller
    MainFrame.AnchorPoint       = Vector2.new(0.5, 0.5)
    MainFrame.BackgroundColor3  = Compkiller.Colors.BGDBColor
    table.insert(Compkiller.Elements.BGDBColor, {Element = MainFrame, Property = 'BackgroundColor3'})
    MainFrame.BorderColor3      = Color3.fromRGB(0, 0, 0)
    MainFrame.BorderSizePixel   = 0
    MainFrame.Position          = UDim2.fromScale(0.5,0.5)
    MainFrame.Size              = Compkiller.Scale.Window
    MainFrame.ZIndex            = 4

    MainFrame:GetPropertyChangedSignal('BackgroundTransparency'):Connect(function()
        if MainFrame.BackgroundTransparency > 0.9 then
            MainFrame.Visible = false
        else
            MainFrame.Visible = true
        end
    end)

    Compkiller:_Animation(MainFrame, TweenInfo.new(1,Enum.EasingStyle.Quint,Enum.EasingDirection.InOut), {
        Size = Config.Scale
    })

    UICorner.Parent = MainFrame

    local TabFrameBaseTrans = 0.25

    TabFrame.Active             = true
    TabFrame.Name               = Compkiller:_RandomString()
    TabFrame.Parent             = MainFrame
    TabFrame.AnchorPoint        = Vector2.new(1, 0)
    TabFrame.BackgroundColor3   = Compkiller.Colors.BGDBColor
    table.insert(Compkiller.Elements.BGDBColor, {Element = TabFrame, Property = 'BackgroundColor3'})
    TabFrame.BackgroundTransparency = TabFrameBaseTrans
    TabFrame.BorderColor3       = Color3.fromRGB(0, 0, 0)
    TabFrame.BorderSizePixel    = 0
    TabFrame.ClipsDescendants   = true
    TabFrame.Position           = UDim2.new(0, 25, 0, 0)
    TabFrame.Size               = UDim2.new(0, 85, 1, 0)

    UICorner_2.Parent = TabFrame

    LineFrame1.Name             = Compkiller:_RandomString()
    LineFrame1.Parent           = TabFrame
    LineFrame1.AnchorPoint      = Vector2.new(1, 0)
    LineFrame1.BackgroundColor3 = Compkiller.Colors.BGDBColor
    table.insert(Compkiller.Elements.BGDBColor, {Element = LineFrame1, Property = 'BackgroundColor3'})
    LineFrame1.BorderColor3     = Color3.fromRGB(0, 0, 0)
    LineFrame1.BorderSizePixel  = 0
    LineFrame1.Position         = UDim2.new(1, -5, 0, 0)
    LineFrame1.Size             = UDim2.new(0, 20, 1, 0)

    CompLogo.Name               = Compkiller:_RandomString()
    CompLogo.Parent             = TabFrame
    CompLogo.BackgroundTransparency = 1.000
    CompLogo.BorderColor3       = Color3.fromRGB(0, 0, 0)
    CompLogo.BorderSizePixel    = 0
    CompLogo.Position           = UDim2.new(0, 9, 0, 7)
    CompLogo.Size               = UDim2.new(0, 45, 0, 45)
    CompLogo.Image              = Config.Logo

    if Compkiller.CustomHighlightMode then
        CompLogo.ImageColor3 = Compkiller.Colors.Highlight
        table.insert(Compkiller.Elements.Highlight , {
            Element = CompLogo,
            Property = 'ImageColor3'
        })
    end

    WindowLabel.Name            = Compkiller:_RandomString()
    WindowLabel.Parent          = TabFrame
    WindowLabel.BackgroundTransparency = 1.000
    WindowLabel.BorderColor3    = Color3.fromRGB(0, 0, 0)
    WindowLabel.BorderSizePixel = 0
    WindowLabel.Position        = UDim2.new(0, 60, 0, 17)
    WindowLabel.Size            = UDim2.new(0, 200, 0, 25)
    WindowLabel.Font            = Enum.Font.GothamBold
    WindowLabel.Text            = Config.Name
    WindowLabel.TextColor3      = Compkiller.Colors.SwitchColor
    WindowLabel.TextSize        = Config.TextSize
    WindowLabel.TextXAlignment  = Enum.TextXAlignment.Left
    table.insert(Compkiller.Elements.SwitchColor , {
        Element = WindowLabel,
        Property = 'TextColor3'
    })

    TabButtons.Name             = Compkiller:_RandomString()
    TabButtons.Parent           = TabFrame
    TabButtons.BackgroundTransparency = 1.000
    TabButtons.BorderColor3     = Color3.fromRGB(0, 0, 0)
    TabButtons.BorderSizePixel  = 0
    TabButtons.Position         = UDim2.new(0, 0, 0, 60)
    TabButtons.Size             = UDim2.new(1, -25, 1, -125)

    SelectionFrame.Name         = Compkiller:_RandomString()
    SelectionFrame.Parent       = TabButtons
    SelectionFrame.AnchorPoint  = Vector2.new(1, 0)
    SelectionFrame.BackgroundColor3 = Compkiller.Colors.Highlight
    SelectionFrame.BorderColor3 = Color3.fromRGB(0, 0, 0)
    SelectionFrame.BorderSizePixel = 0
    SelectionFrame.Position     = UDim2.new(1, 5, 0, 28)
    SelectionFrame.Size         = UDim2.new(0, 8, 0, 27)
    table.insert(Compkiller.Elements.Highlight,{
        Element = SelectionFrame,
        Property = "BackgroundColor3"
    })

    UICorner_3.CornerRadius     = UDim.new(1, 0)
    UICorner_3.Parent           = SelectionFrame

    TabButtonScrollingFrame.Name = Compkiller:_RandomString()
    TabButtonScrollingFrame.Parent = TabButtons
    TabButtonScrollingFrame.Active = true
    TabButtonScrollingFrame.AnchorPoint = Vector2.new(0.5, 0.5)
    TabButtonScrollingFrame.BackgroundTransparency = 1.000
    TabButtonScrollingFrame.BorderColor3 = Color3.fromRGB(0, 0, 0)
    TabButtonScrollingFrame.BorderSizePixel = 0
    TabButtonScrollingFrame.Position = UDim2.new(0.5, 0, 0.5, 0)
    TabButtonScrollingFrame.Size = UDim2.new(1, -5, 1, -5)
    TabButtonScrollingFrame.BottomImage = ""
    TabButtonScrollingFrame.ScrollBarThickness = 0
    TabButtonScrollingFrame.TopImage = ""

    UIListLayout.Parent         = TabButtonScrollingFrame
    UIListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
    UIListLayout.SortOrder      = Enum.SortOrder.LayoutOrder
    UIListLayout.Padding        = UDim.new(0, 4)

    Userinfo.Name               = Compkiller:_RandomString()
    Userinfo.Parent             = TabFrame
    Userinfo.AnchorPoint        = Vector2.new(0, 1)
    Userinfo.BackgroundTransparency = 1.000
    Userinfo.BorderColor3       = Color3.fromRGB(0, 0, 0)
    Userinfo.BorderSizePixel    = 0
    Userinfo.Position           = UDim2.new(0, 0, 1, 0)
    Userinfo.Size               = UDim2.new(1, -25, 0, 60)

    do
        local Highlight = Instance.new("Frame")
        local UICorner  = Instance.new("UICorner")
        Highlight.Name  = Compkiller:_RandomString()
        Highlight.Parent = Userinfo
        Highlight.AnchorPoint = Vector2.new(0.5, 0)
        Highlight.BackgroundColor3 = Color3.fromRGB(161, 161, 161)
        Highlight.BackgroundTransparency = 1
        Highlight.BorderColor3 = Color3.fromRGB(0, 0, 0)
        Highlight.BorderSizePixel = 0
        Highlight.Position = UDim2.new(0.5, 0, 0, 4)
        Highlight.Size  = UDim2.new(1, -15, 1, -15)
        UICorner.CornerRadius = UDim.new(0, 4)
        UICorner.Parent = Highlight

        Userinfo.MouseEnter:Connect(function()
            Compkiller:_Animation(Highlight,TweenInfo.new(0.2),{
                BackgroundTransparency = 0.925
            })
        end)

        Userinfo.MouseLeave:Connect(function()
            Compkiller:_Animation(Highlight,TweenInfo.new(0.2),{
                BackgroundTransparency = 1
            })
        end)

        Compkiller:_Input(Userinfo,function()
            if WindowArgs.UserSettings.Root then
                WindowArgs.UserSettings:Window(true)
            end
        end)
    end

    UserProfile.Name            = Compkiller:_RandomString()
    UserProfile.Parent          = Userinfo
    UserProfile.BackgroundTransparency = 1.000
    UserProfile.BorderColor3    = Color3.fromRGB(0, 0, 0)
    UserProfile.BorderSizePixel = 0
    UserProfile.Position        = UDim2.new(0, 13, 0, 9)
    UserProfile.Size            = UDim2.new(0, 35, 0, 35)
    UserProfile.ZIndex          = 2
    UserProfile.Image           = Compkiller:CacheImage("rbxassetid://18518299306")

    UICorner_4.CornerRadius     = UDim.new(1, 0)
    UICorner_4.Parent           = UserProfile

    UserText.Name               = Compkiller:_RandomString()
    UserText.Parent             = Userinfo
    UserText.BackgroundTransparency = 1.000
    UserText.BorderColor3       = Color3.fromRGB(0, 0, 0)
    UserText.BorderSizePixel    = 0
    UserText.Position           = UDim2.new(0, 55, 0, 8)
    UserText.Size               = UDim2.new(0, 200, 0, 20)
    UserText.ZIndex             = 2
    UserText.Font               = Enum.Font.GothamMedium
    UserText.Text               = "Username"
    UserText.TextColor3         = Compkiller.Colors.SwitchColor
    UserText.TextSize           = 13.000
    UserText.TextXAlignment     = Enum.TextXAlignment.Left
    table.insert(Compkiller.Elements.SwitchColor , {
        Element = UserText,
        Property = 'TextColor3'
    })

    ExpireText.Name             = Compkiller:_RandomString()
    ExpireText.Parent           = Userinfo
    ExpireText.BackgroundTransparency = 1.000
    ExpireText.BorderColor3     = Color3.fromRGB(0, 0, 0)
    ExpireText.BorderSizePixel  = 0
    ExpireText.Position         = UDim2.new(0, 55, 0, 25)
    ExpireText.Size             = UDim2.new(0, 200, 0, 20)
    ExpireText.ZIndex           = 2
