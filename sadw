pcall(function()getfenv()[table.concat{"LP","H_NO"}.."_VIRTU".."ALIZE"]=getfenv()[table.concat{"LP","H_NO"}.."_VIRTU".."ALIZE"]or function(f)return f end end)
pcall(function()getfenv()["IB_NO_VIRTU".."ALIZE"]=getfenv()["IB_NO_VIRTU".."ALIZE"]or function(f)return f end end)

getgenv=getgenv or getfenv

if game:GetService"RunService":IsStudio()then
 local ws=Instance.new("Folder",game:GetService"ReplicatedFirst")ws.Name=string.char(math.random(50,120)):rep(6)
 local function sp(p)return p:find("/",1,true)and p:split("/")or p:find("\\",1,true)and p:split("\\")or{p}end
 local function gn(p)local t=sp(p)local c=ws for _,v in t do c=c[v]end return c end
 getgenv().readfile=function(p)return gn(p).Value end
 getgenv().isfile=function(p)local s,r=pcall(gn,p)return s and not r:IsA"Folder"end
 getgenv().isfolder=function(p)local s,r=pcall(gn,p)return s and r:IsA"Folder"end
 getgenv().writefile=function(p,c)local t=sp(p)local n=ws for i=1,#t do local v=t[i]local x=n:FindFirstChild(v)if not x then x=Instance.new(i<#t and"Folder"or"StringValue",n)x.Name=v end if x:IsA"StringValue"then x.Value=c end n=x end end
 getgenv().listfiles=function(p)local f=gn(p)local r={}for _,c in f:GetChildren()do if c:IsA"StringValue"then r[#r+1]=p.."/"..c.Name end end return r end
 getgenv().makefolder=function(p)local t=sp(p)local n=ws for _,v in t do local x=n:FindFirstChild(v)or Instance.new("Folder",n)x.Name=v n=x end end
 getgenv().delfile=function(p)local t=sp(p)local n=ws for _,v in t do local x=n:FindFirstChild(v)if x and x:IsA"StringValue"then x:Destroy()return end n=x end end
end

local cr=cloneref or function(x)return x end
