function Compkiller:OptimizeMode(v)Compkiller.PerformanceMode=v end
function Compkiller:IsStudio()return RunService:IsStudio()end
function Compkiller:CustomIconHighlight()Compkiller.CustomHighlightMode=true end

function Compkiller:_SetNilP(Ins,Parent)
	Compkiller.WindowsNil=Compkiller.WindowsNil or{}
	Compkiller.NilFolder=Compkiller.NilFolder or Instance.new"Folder"
	if not Compkiller.WindowsNil[Ins]then
		Compkiller.WindowsNil[Ins]=Compkiller:_GetWindowFromElement(Ins)
	end
	Ins.Parent=Parent or Compkiller.NilFolder
end

function Compkiller:SetAllText(flags)
	if not flags then for _,v in Compkiller.Flags do if v.SetText then v:SetText()end end return end
	for i,v in flags do if Compkiller.Flags[i]and Compkiller.Flags[i].SetText then Compkiller.Flags[i]:SetText(v)end end
end

function Compkiller:_GetIcon(name,font_aws)
	if Compkiller.SecureMode then
		local id=font_aws and(Compkiller.FontAwesome[name]or name)or(Compkiller.Lucide["lucide-"..name]or Compkiller.Lucide[name]or Compkiller.Lucide[tostring(name)]or Compkiller.FontAwesome[name]or name)
		return id and Compkiller:CacheImage(id)or""
	end
	return font_aws and(Compkiller.FontAwesome[name]or name)or(Compkiller.Lucide["lucide-"..name]or Compkiller.Lucide[name]or Compkiller.Lucide[tostring(name)]or Compkiller.FontAwesome[name]or name)
end

function Compkiller:_RandomString()return"CK="..string.char(math.random(64,102)):rep(20)end

function Compkiller:_IsMouseOverFrame(f)
	if not f then return end
	local p,s=f.AbsolutePosition,f.AbsoluteSize
	return Mouse.X>=p.X and Mouse.X<=p.X+s.X and Mouse.Y>=p.Y and Mouse.Y<=p.Y+s.Y
end

function Compkiller:_Rounding(n,d) local m=10^(d or 0)return math.floor(n*m+0.5)/m end

function Compkiller:_Animation(o,i,p)local t=TweenService:Create(o,i or TweenInfo.new(.25),p)t:Play()return t end

function Compkiller:_Input(f,cb)local b=Instance.new("TextButton",f)b.ZIndex=f.ZIndex+10 b.Size=UDim2.fromScale(1,1)b.BackgroundTransparency=1 b.TextTransparency=1 if cb then b.MouseButton1Click:Connect(cb)end return b end

function Compkiller:GetCalculatePosition(planePos,planeNormal,rayOrigin,rayDirection)
	local n,d,v=planeNormal,rayDirection,rayOrigin-planePos
	return rayOrigin+(-(n:Dot(v))/n:Dot(d))*d
end

function Compkiller:_Blur(el,winRemote)
	if Compkiller.SecureMode and not Compkiller.SecurityConfig.BlurEnabled then return game.Changed:Connect(function()end)end

	local p=Instance.new("Part",Compkiller.ArcylicParent)local dof=Instance.new("DepthOfFieldEffect",game.Lighting)local m=Instance.new("BlockMesh",p)
	p.Material=Enum.Material.Glass p.Transparency=1 p.Reflectance=1 p.CastShadow=false p.Anchored=true p.CanCollide=false p.CanQuery=false p.CollisionGroup=Compkiller:_RandomString()p.Size=Vector3.new(.01,.01,.01)p.Color=Color3.new()

	dof.Enabled=true dof.FarIntensity=0 dof.FocusDistance=0 dof.InFocusRadius=1000 dof.NearIntensity=1 dof.Name=Compkiller:_RandomString()

	local dc
	local upd=function()
		if Compkiller.SecureMode then p and p:Destroy()dof and dof:Destroy()m and m:Destroy()dc and dc()return end
		local act=winRemote:GetValue()
		if act then
			Compkiller:_Animation(dof,TweenInfo.new(.1),{NearIntensity=1})
			Compkiller:_Animation(p,TweenInfo.new(.1),{Transparency=.97,Size=Vector3.new(.01,.01,.01)})
		else
			Compkiller:_Animation(dof,TweenInfo.new(.1),{NearIntensity=0})
			Compkiller:_Animation(p,TweenInfo.new(.1),{Size=Vector3.zero,Transparency=1.5})
			return false
		end
		if act then
			local c0,c1=el.AbsolutePosition,el.AbsolutePosition+el.AbsoluteSize
			local r0,r1=CurrentCamera:ScreenPointToRay(c0.X,c0.Y,1),CurrentCamera:ScreenPointToRay(c1.X,c1.Y,1)
			local po=CurrentCamera.CFrame.Position+CurrentCamera.CFrame.LookVector*(.05-CurrentCamera.NearPlaneZ)
			local pn=CurrentCamera.CFrame.LookVector
			local p0,p1=Compkiller:GetCalculatePosition(po,pn,r0.Origin,r0.Direction),Compkiller:GetCalculatePosition(po,pn,r1.Origin,r1.Direction)
			p0,p1=CurrentCamera.CFrame:PointToObjectSpace(p0),CurrentCamera.CFrame:PointToObjectSpace(p1)
			local sz,ctr=(p1-p0),(p0+p1)/2
			m.Offset=ctr m.Scale=sz/0.0101 p.CFrame=CurrentCamera.CFrame
		end
	end

	local cs=CurrentCamera:GetPropertyChangedSignal("CFrame"):Connect(upd)
	local ic=UserInputService.InputChanged:Connect(function(i)if i.UserInputType==Enum.UserInputType.MouseMovement or i.UserInputType==Enum.UserInputType.Touch then pcall(upd)end end)
	local th=task.spawn(function()while true do task.wait(.1)pcall(upd)end end)

	dc=function()cs:Disconnect()ic:Disconnect()task.cancel(th)p:Destroy()dof:Destroy()end
	el.Destroying:Connect(dc)
	return cs
end

function Compkiller:_AddDragBlacklist(f)
	local added=false
	local setb=function(v)
		local i=table.find(Compkiller.DragBlacklist,f)
		if v and not Compkiller.IS_DRAG_MOVE then if not i then table.insert(Compkiller.DragBlacklist,f)end else if i then table.remove(Compkiller.DragBlacklist,i)end end
	end
	f.InputBegan:Connect(function()if Compkiller:_IsMouseOverFrame(f)then setb(true)end end)
	f.InputEnded:Connect(function()setb(false)end)
	UserInputService.InputChanged:Connect(function()if not Compkiller:_IsMouseOverFrame(f)then setb(false)end end)
end

function Compkiller:_GetWindowFromElement(el)
	if Compkiller.WindowsNil[el]then return Compkiller.WindowsNil[el]end
	for _,w in Compkiller.Windows do if el:IsDescendantOf(w)then return w end end
	for fr,win in Compkiller.WindowsNil do if el:IsDescendantOf(fr)or fr==el then return win end end
end

function Compkiller.__SIGNAL(def)
	local be=Instance.new"BindableEvent"be:SetAttribute("Value",def)
	local sig={__signals={}}
	function sig:Connect(fn)fn(be:GetAttribute"Value")local c=be.Event:Connect(fn)table.insert(sig.__signals,c)return c end
	function sig:Fire(v)local same=be:GetAttribute"Value"==v be:SetAttribute("Value",v)if not same then be:Fire(v)end end
	function sig:GetValue()return be:GetAttribute"Value"end
	return sig
end

function Compkiller:_Hover(f,on,off)f.MouseEnter:Connect(on)f.MouseLeave:Connect(off)end

function Compkiller.__CONFIG(c,d)for k,v in d do if c[k]==nil then c[k]=v end end return c end

function Compkiller:Drag(inp,mv,speed)
	local tog,st,sp=false,nil,nil local ti=TweenInfo.new(speed or .2)
	local up=function(i)local d=i.Position-st local p=UDim2.new(sp.X.Scale,sp.X.Offset+d.X,sp.Y.Scale,sp.Y.Offset+d.Y)Compkiller:_Animation(mv,ti,{Position=p})end
	inp.InputBegan:Connect(function(i)
		if(i.UserInputType==Enum.UserInputType.MouseButton1 or i.UserInputType==Enum.UserInputType.Touch)and#Compkiller.DragBlacklist<1 then
			tog=true st=i.Position sp=mv.Position
			i.Changed:Connect(function()if i.UserInputState==Enum.UserInputState.End then tog=false Compkiller.IS_DRAG_MOVE=false end end)
		end
		if not Compkiller.IsDrage and tog then Compkiller.LastDrag=tick()end
		Compkiller.IaDrag=tog
	end)
	UserInputService.InputChanged:Connect(function(i)
		if(i.UserInputType==Enum.UserInputType.MouseMovement or i.UserInputType==Enum.UserInputType.Touch)and#Compkiller.DragBlacklist<1 then
			if tog then Compkiller.IS_DRAG_MOVE=true up(i)else Compkiller.IS_DRAG_MOVE=false end
		elseif#Compkiller.DragBlacklist>0 then tog=false Compkiller.IS_DRAG_MOVE=false end
		Compkiller.IaDrag=tog
	end)
end

function Compkiller:_IsMobile()return UserInputService.TouchEnabled end

function Compkiller:Hash(s)
	if typeof(s)~="string"then return"ck-unknow"end
	local h=#s s:gsub(".",function(b)h=h+b:byte()+#s end)
	return"ck-"..math.round(h+15)..(tonumber(s:match"%d+")or"")
end

function Compkiller:CacheImage(id)
	if not Compkiller.SecureMode or not id or not id:byte()then return id or""end
	assert(Compkiller.CacheDirectory,"call :Security() first")
	local num=id:match"%d+"
	if not num then return id end
	local h=Compkiller:Hash(id)local p=Compkiller.CacheDirectory.."/cache-"..h..".png"
	if isfile(p)then return(getcustomasset or getsynasset)(p)end
	local sz=Compkiller.SecurityConfig.ImageScale or 150
	local url=("https://thumbnails.roblox.com/v1/assets?assetIds=%s&size=%sx%s&format=Png&isCircular=false"):format(num,sz,sz)
	local r=game:HttpGet(url)local d=HttpService:JSONDecode(r)
	if d and d.data and d.data[1]and d.data[1].imageUrl then
		writefile(p,game:HttpGet(d.data[1].imageUrl))
		return(getcustomasset or getsynasset)(p)
	end
	return""
end

function Compkiller:PreloadIcons()
	local t={"rbxassetid://112554223509763","rbxassetid://4805639000","rbxassetid://6198493000","rbxassetid://10709790948","rbxassetid://18518299306","rbxassetid://10747362393","rbxassetid://18720640102","rbxassetid://10723344270","rbxassetid://109535175596957","rbxassetid://10747384394","rbxassetid://10734941499",Compkiller.Logo}
	if Compkiller.SecureMode then for _,v in t do task.wait()pcall(Compkiller.CacheImage,Compkiller,v)end else game:GetService"ContentProvider":PreloadAsync(t)end
end

function Compkiller:Security(dir,cfg)
	dir=dir or"Compkiller-Cache"
	if not isfolder(dir)then makefolder(dir)end
	Compkiller.SecureMode=true Compkiller.CacheDirectory=dir Compkiller.SecurityConfig=cfg or{}
end
